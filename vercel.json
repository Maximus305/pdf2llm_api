{
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "api/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ]
}