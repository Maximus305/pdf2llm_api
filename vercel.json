{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/app.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ]
}